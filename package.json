{
  "name": "ember-slide-push-menu",
  "version": "1.1.1",
  "description": "Fixed menus that will slide out from the sides of the page and in case of the right and left side optionally move the body.",
  "keywords": [
    "ember-addon",
    "menu",
    "navigation",
    "slide-menu",
    "push-menu",
    "off-canvas"
  ],
  "license": "MIT",
  "author": "Rajasegar Chandran",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/rajasegar/ember-slide-push-menu",
  "scripts": {
    "build": "ember build --environment=production",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint . ",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "start": "ember server",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "COVERAGE=true ember test",
    "test:ember-compatibility": "ember try:each",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "ember-cli-babel": "^7.22.1",
    "ember-cli-htmlbars": "^5.3.1"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/render-modifiers": "^1.0.2",
    "@glimmer/component": "^1.0.1",
    "@glimmer/tracking": "^1.0.1",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "coveralls": "^3.1.0",
    "ember-addon-starterkit": "^0.1.11",
    "ember-auto-import": "^1.6.0",
    "ember-cli": "~3.21.0",
    "ember-cli-code-coverage": "^1.0.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-qunit": "^4.4.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.2",
    "ember-source": "^3.21.1",
    "ember-template-lint": "^2.11.0",
    "eslint": "^7.8.0",
    "eslint-plugin-node": "^11.1.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "semantic-release": "^17.1.1"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://rajasegar.github.io/ember-slide-push-menu"
  }
}
